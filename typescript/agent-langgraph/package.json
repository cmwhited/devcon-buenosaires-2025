{
  "name": "agent-langgraph",
  "version": "1.0.0",
  "description": "LangGraph.js agent with x402-enabled MCP tool integration",
  "type": "module",
  "scripts": {
    "dev": "pnpm tsx --watch ./src/agent.ts",
    "build": "tsdown",
    "start": "node ./dist/agent.mjs",
    "format": "prettier . --write --list-different --ignore-path ../../.prettierignore",
    "format:check": "prettier . --check --ignore-path ../../.prettierignore",
    "lint": "eslint \"**/src/**/*.{ts,mjs}\"",
    "lint:fix": "pnpm lint --fix",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run typecheck; pnpm run lint; pnpm run format"
  },
  "dependencies": {
    "@edgeandnode/ampersend-sdk": "workspace:^",
    "@langchain/core": "^1.0.6",
    "@langchain/langgraph": "^1.0.2",
    "@langchain/mcp-adapters": "^1.0.0",
    "@langchain/openai": "^1.1.2",
    "@modelcontextprotocol/sdk": "github:edgeandnode/mcp-typescript-sdk#2de06543904483073d8cc13db1d0e08e16601081",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "tsdown": "^0.16.6"
  }
}
